{
  "name": "iat-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng-build": "ng build",
    "dev": "ng build --output-path=build/resources/main/static --watch",
    "ng-build-prod": "ng build --env=prod",
    "start": "ng serve  --proxy-config proxy.conf.json",
    "start-prod": "ng serve --proxy-config proxy.conf.json --prod",
    "lint": "ng lint",
    "lint-on-ci": "tslint \"src/**/*.ts\" --project src/tsconfig.app.json --formatters-dir node_modules/tslint-teamcity-reporter/ --format TSHintTeamcity file.ts",
    "test": "ng test",
    "test-on-ci": "ng test -sr -cc --browsers PhantomJS",
    "code-coverage": "ng test --single-run --code-coverage",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ng-idle/core": "^2.0.0-beta.12",
    "@ng-idle/keepalive": "^2.0.0-beta.12",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "json2typescript": "^0.9.5",
    "ng2-toasty": "^4.0.0",
    "ngx-bootstrap": "^1.6.6",
    "rxjs": "^5.1.0",
    "@sbac/sbac-ui-kit": "0.0.17",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.4",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.1.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.6.0",
    "karma-teamcity-reporter": "^1.0.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "tslint-teamcity-reporter": "^2.0.0",
    "typescript": "~2.2.0"
  }
}
